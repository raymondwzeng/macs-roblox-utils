{
    "name":"Mac's Roblox Utils",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "A collection of neat stuff to enchance your Roblox website experience.",
    "author": "MacAndSwiss",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png"
    },
    "content_scripts": [{
        "matches": ["*://*.roblox.com/*"],
        "js": ["content.js"],
        "run_at":"document_end"
    }],
    "action": {
        "default_title": "Launch Mac's Roblox Util",
        "default_popup": "Main.html"
    },
    "background": {
        "service_worker": "backgroundtest.js"
    },
    "host_permissions":["https://*.roblox.com/"],
    "permissions":["storage", "webNavigation", "tabs"]
}